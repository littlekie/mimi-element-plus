{
  "name": "mini-element-plus",
  "packageManager": "pnpm@7.9.0",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:serve": "pnpm run -C docs serve"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "dependencies": {
    "@littlekie/components": "workspace:*",
    "@littlekie/hooks": "workspace:*",
    "@littlekie/utils": "workspace:*",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vitest/ui": "^0.22.1",
    "@vue/test-utils": "^2.0.2",
    "c8": "^7.12.0",
    "chalk": "^4.1.0",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.2.1",
    "sass": "^1.54.4",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vitest": "^0.21.0",
    "vue-tsc": "^0.38.4",
    "yorkie": "^2.0.0"
  }
}